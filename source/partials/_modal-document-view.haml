.modal.fade.modal-document-image{tabindex: "-1", role: "dialog"}
  .modal-dialog{role:"document"}
    .modal-content

      %button.close.btn-close{type: "button", data:{dismiss:"modal"}, aria:{label: "Close"}}
        %span{aria:{hidden:"true"}} &times;

      .modal-body
        %section.modal-sidebar

          %h4.section-header.sh-short Other Load Documents
          .load-documents
            .load-doc
              .doc-image
                %a{href: "#"}
                  =image_tag('document_image.png')
              .doc-descrip
                %h5 Rate Confirmation
                .doc-data
                  Updated 06:45 CST, Today
                  %br
                  By Echo

            .load-doc
              .doc-image
                %a{href: "#"}
                  =image_tag('doc-TEMP-image.png')
              .doc-descrip
                %h5 Proof of Delivery
                .doc-data
                  Updated 06:45 CST, Today
                  %br
                  By Michelle P.

          %button.btn-link{type: 'button'} Confirm Arrival
          %button.btn-link{type: "button", data: {dismiss: 'modal'}} CANCEL

        %section.modal-main-content
          %ul.list-inline.pull-right
            %li
              =link_to 'Download Document', '#'
            %li
              =link_to 'Print', '#'
            %li
              =link_to 'Add Page', '#'





